{
  "name": "chess-admin-server",
  "version": "0.1.0",
  "description": "chess admin server api",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "build": "gulp init && gulp build && docker build -t 192.168.221.39:5000/cuiweiqiang/chess-admin-server:0.1.0 .",
    "deploy": "docker push 192.168.221.39:5000/cuiweiqiang/chess-admin-server:0.1.0",
    "tsc:w": "tsc -p tsconfig.json -w",
    "start": "egg-scripts start ./dist",
    "stop": "egg-scripts stop",
    "dev": "egg-bin dev --baseDir ./dist",
    "debug": "egg-bin debug --baseDir ./dist",
    "test": "TESTS=dist/test egg-bin test ",
    "test:lint": "npm run lint -- --fix && npm run test",
    "cov": "egg-bin cov ./dist/test",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "keywords": [
    "chess",
    "admin",
    "api"
  ],
  "author": "Cui",
  "license": "ISC",
  "dependencies": {
    "@ineentho/mongodb-autoincrement": "^1.0.2",
    "await-stream-ready": "^1.0.1",
    "bcrypt": "^1.0.3",
    "bcryptjs": "^2.4.3",
    "egg": "^2.3.0",
    "egg-cors": "^2.0.0",
    "egg-jwt": "^2.2.1",
    "egg-lru": "^1.0.1",
    "egg-mongo-native": "^3.0.0",
    "egg-mongoose": "^2.1.1",
    "egg-oss": "^1.1.0",
    "egg-scripts": "^2.5.0",
    "egg-validate": "^1.0.0",
    "gd-bmp": "^1.2.4",
    "ioredis": "^3.2.2",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.1",
    "moment": "^2.20.1",
    "mongoose": "^4.13.9",
    "mongoose-delete": "^0.4.0",
    "mongoose-findorcreate": "^2.0.0",
    "mongoose-friends": "^0.2.5",
    "mongoose-query": "^0.5.1",
    "nanoid": "^1.0.1",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "sf": "^0.1.9",
    "shortid": "^2.2.8",
    "stream-wormhole": "^1.0.3",
    "svg-captcha": "^1.3.11",
    "tslib": "^1.9.0",
    "uuid": "^3.2.1",
    "ylru": "^1.2.0",
    "yunpian-sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.0.2",
    "@types/node": "^9.4.0",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.1",
    "del": "^3.0.0",
    "egg-bin": "^4.3.7",
    "egg-mock": "^3.14.0",
    "gulp": "^3.9.1",
    "gulp-aglio": "0.0.10",
    "gulp-git": "^2.5.2",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^4.0.1",
    "pre-commit": "^1.2.2",
    "run-sequence": "^2.2.1",
    "typescript": "^2.8.0-dev.20180221"
  },
  "pre-commit": [
    "precommit-msg"
  ]
}
